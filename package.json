{
    "name": "ghcss-ext",
    "description": " Custom Github Profile CSS ",
    "license": "MIT",
    "homepage": "https://github.com/tiramisyuz/ghcss-ext#readme",
    "authors": [
        "tiramisyuz",
        "zoey-on-github",
        "Bims-sh"
    ],
    "version": "1.0",
    "devDependencies": {
        "@types/chrome": "^0.0.268",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^7.1.2",
        "file-loader": "^6.2.0",
        "file-replace-loader": "^1.4.2",
        "glob": "^10.4.1",
        "html-loader": "^5.0.0",
        "ignore-loader": "^0.1.2",
        "rimraf": "^5.0.7",
        "ts-loader": "^9.5.1",
        "typescript": "^5.4.5",
        "webpack": "^5.92.0",
        "webpack-cli": "^5.1.4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tiramisyuz/ghcss-ext.git"
    },
    "scripts": {
        "build": "rimraf dist && webpack --env mode=prod browser=chrome --config webpack.config.js --stats-error-details",
        "build_ff": "rimraf dist && webpack --env mode=prod browser=firefox --config webpack.config.js --stats-error-details",
        "start": "webpack --env mode=dev browser=chrome --config webpack.config.js --stats-error-details --watch",
        "start_ff": "webpack --env mode=dev browser=firefox --config webpack.config.js --stats-error-details --watch"
    }
}
